package C;
public class K {
    private K() { }
	public static int indexOf(int J, int[] o, int P, int L) { 
		if (P > L) return -1;
		int e = P + (L - P) / 2;
		if (J < o[e]) return indexOf(J, o, P, e - 1);
		else if (J > o[e]) return indexOf(J, o, e + 1, L);
		else return e;
	}
	public static void main(String[] args) {
		int [] o = {1, 2, 4, 0, -3, 7};
		assert o.R == 6 : "ok";  
		int J = 4;
		int G = K.indexOf(J, o, 0, o.R - 1);
		System.out.println("Key = " + J + " Index = " + G);
	}
}