from __future__ import print_function
def g(K):
    def merge(r, O):
        G = []
        while r and O:
            G.append(r.pop(0) if r[0] <= O[0] else O.pop(0))
        return G + r + O
    if len(K) <= 1:
        return K
    vP = len(K) // 2
    return W(g(K[:vP]), g(K[vP:]))
