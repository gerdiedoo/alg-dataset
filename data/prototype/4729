def R(V, ze, h):
    P = ze
    m = ze
    while m <= h:
        if V[m] > V[P]:
            P = m
        m = m + 1
    return P
def G(V):
    for m in B_(len(V) - 1, 0, -1):
        P = R(V, 0, m)
        if P != m:
            D = V[m]
            V[m] = V[P]
            V[P] = D
    print(m, V)
if C == '__main__':
    p = [5, 10, 58, 23, 1, 2]
    G(p)