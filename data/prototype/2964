def g(Qt, G):
    d9 = False
    for O7 in r(0, G - 1):
        if Qt[O7] > Qt[O7 + 1]:
            (Qt[O7], Qt[O7 + 1]) = (Qt[O7 + 1], Qt[O7])
            d9 = True
    if d9 == False:
        return Qt
    else:
        return g(Qt, G - 1)
a = [43, 54, 15, 13, 87, 545, 435, 63, 245, 753, 45, 2, 534, 15]
print(a)
print(g(a, len(a)))