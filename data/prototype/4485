def I(v, c, Q):
    g = v[Q]
    N = c - 1
    for A in f(c, Q):
        if v[A] < g:
            (v[A], v[N]) = (v[N], v[A])
            N += 1
    (v[Q], v[N + 1]) = (v[N + 1], v[Q])
    return N + 1
def K(v, c, Q):
    if c < Q:
        z = I(v, c, Q)
        K(v, c, z - 1)
        K(v, z + 1, c)
def U():
    x = [10, 2, 5, 8, 1]
    K(x, 0, len(x) - 1)
    print(x)
    print('Hello World!')
if e == '__main__':
    U()