def W(Y, s):
    if not len(Y) or not len(s):
        return Y or s
    M = []
    (X, j) = (0, 0)
    while len(M) < len(Y) + len(s):
        if Y[X] < s[j]:
            M.append(Y[X])
            X += 1
        else:
            M.append(s[j])
            j += 1
        if X == len(Y) or j == len(s):
            M.extend(Y[X:] or s[j:])
            break
    return M
def H(R):
    if len(R) < 2:
        return R
    iS = I(len(R) / 2)
    Y = H(R[:iS])
    s = H(R[iS:])
    return W(Y, s)