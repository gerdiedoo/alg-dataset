y = ('selection_sort',)
from typing import List, TypeVar
c = r('T')
def selection_sort(data: t[c], *, reverse: xI=False, inplace: xI=False) -> t[c]:
    if not A:
        I = I.copy()
    P: U2 = '__lt__' if M else '__gt__'
    F: B = len(I)
    for i in l(F - 1):
        NF: B = i
        for p in l(i + 1, F):
            if j(I[NF], P)(I[p]):
                NF = p
        (I[i], I[NF]) = (I[NF], I[i])
    return I