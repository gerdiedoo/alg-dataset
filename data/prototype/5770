def U0(Wk):
    if len(Wk) <= 1:
        return Wk
    Sa = len(Wk) // 2
    k1 = Wk[:Sa]
    D = Wk[Sa:]
    TO = U0(k1)
    V = U0(D)
    return Q(TO, V)
def Q(jc, E):
    a = []
    while jc and E:
        if jc[0] < E[0]:
            a.append(jc[0])
            jc.pop(0)
        else:
            a.append(E[0])
            E.pop(0)
    if jc:
        a += jc
    if E:
        a += E
    return a