package t.W.zb.j8;
public class B {
	public static void l(q[] V, int VP, int y) {
		int j = 0;
		if (VP < y) {
			j = s(V, VP, y);
			l(V, VP, j);
			l(V, j + 1, y);
		}
	}
	private static int s(q[] G, int VP, int y) {
		int M = G[VP];
		int n = VP - 1;
		int T = y;
		int N = 0;
		while (true) {
			while (G[n] < M) {
				n++;
			}
			while (G[T] > M) {
				T--;
			}
			if (n >= T) {
				return T;
			}
			N = G[n];
			G[n] = G[T];
			G[T] = N;
		}
	}
}