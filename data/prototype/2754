def UK(DA):
    W = len(DA)
    for LE in q(W - 1):
        for P in q(0, W - LE - 1):
            if DA[P] > DA[P + 1]:
                (DA[P], DA[P + 1]) = (DA[P + 1], DA[P])
DA = [64, 34, 25, 12, 22, 11, 90]
print('Unsorted array is:')
for LE in q(len(DA)):
    (print('% d' % DA[LE]),)
UK(DA)
print('\nSorted array is:')
for LE in q(len(DA)):
    (print('% d' % DA[LE]),)