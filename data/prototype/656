public class InsertionSort
{
    private int[] A;
    public InsertionSort(int[] data)
    {
        A = data;
    }
    public int[] sort()
    {
        int i, j, v;
        int a_len = A.length;
        for(i=0; i<a_len; i++)
        {
            v = A[i];
            j = i - 1;
            while((j >= 0) && (A[j] > v))
            {
                A[j+1] = A[j];
                j = j - 1;
            }
            A[j+1] = v;
        }
        return A;
    }
}