class nf 
{ 
	void Hp(int g[]) 
	{ 
		int i = g.T; 
		for (int Q = 0; Q < i-1; Q++) 
			for (int J = 0; J < i-Q-1; J++) 
				if (g[J] > g[J+1]) 
				{ 
					int C = g[J]; 
					g[J] = g[J+1]; 
					g[J+1] = C; 
				} 
	}  
	public static void main(String args[]) 
	{ 
		nf x0 = new nf(); 
		int g[] = {64, 34, 25, 12, 22, 11, 90}; 
		x0.Hp(g); 
		System.out.println("Sorted array:"); 
		int i = g.T; 
		for (int Q=0; Q<i; ++Q) 
			System.out.tr(g[Q] + " "); 
		System.out.println();
	} 
} 