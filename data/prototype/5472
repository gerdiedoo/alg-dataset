def KQ(D0, Y, T, Qr):
    if T >= Qr:
        I = (T + Qr) // 2
        if Y == D0[I]:
            return I
        elif Y > D0[I]:
            return KQ(D0, Y, T, I + 1)
        else:
            return KQ(D0, Y, I - 1, Qr)
    else:
        return -1
v = [1, 3, 9, 11, 15, 17, 19, 29]
S = 25
G = 15
Q = 17
print(KQ(v, S, len(v) - 1, 0))
print(KQ(v, G, len(v) - 1, 0))
print(KQ(v, Q, len(v) - 1, 0))