def SelectionSort(A,n):
    for i in range(0,n-1):
        iMin=i
        for j in range(i+1,n):
            if(A[j]<A[iMin]):
                iMin=j
        A[i],A[iMin]=A[iMin],A[i]
if __name__=="__main__":
    A=[]
    print("Enter the elements in array:")
    A=list(map(int,input().split(" ")))
    n=len(A)
    print(n,A)
    SelectionSort(A,n)
    for j in range(0,n):
        print(A[j],end=" ")