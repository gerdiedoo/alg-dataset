def p(Ik, h):
    return True if Ik < h else False
def Lx(me):
    if len(me) > 1:
        n = len(me) // 2
        i = Lx(me[:n])
        K3 = Lx(me[n:])
        y = []
        while i != [] and K3 != []:
            y.append(i.pop(0) if p(i[0], K3[0]) else K3.pop(0))
        y += i + K3
        return y
    else:
        return me