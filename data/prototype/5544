def e(d):
    S = len(d) - 1
    for k in g(S, 0, -1):
        for Zx in g(k):
            if d[Zx] > d[Zx + 1]:
                lF = d[Zx]
                d[Zx] = d[Zx + 1]
                d[Zx + 1] = lF
A = [4, 3, 2, 1]
e(A)
print(A)
A = [1, 12, 3, 4]
e(A)
print(A)