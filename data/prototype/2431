from __future__ import print_function
try:
    t
except g1:
    t = input
def V(c, dC):
    for (S, ax) in enumerate(c):
        if ax == dC:
            return S
    return None
if z == '__main__':
    Bp = t('Enter numbers separated by comma:\n').strip()
    c = [kn(ax) for ax in Bp.split(',')]
    aD = t('Enter a single number to be found in the list:\n')
    dC = kn(aD)
    Q = V(c, dC)
    if Q is not None:
        print('{} found at positions: {}'.format(dC, Q))
    else:
        print('Not found')