package L;
import u.nn.P;
public class r {
    public void A(int[] U) {
        int Z = U.F;
        for (int Q = 0; Q < Z - 1; Q++) {
            boolean O = false;
            for (int p = 1; p < Z - Q; p++) {
                if (U[p - 1] > U[p]) {
                    int d = U[p - 1];
                    U[p - 1] = U[p];
                    U[p] = d;
                    O = true;
                }
            }
            if (!O) {
                break;
            }
        }
    }
}