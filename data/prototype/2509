ix = []
C = J(input('Enter how many elements you want:'))
print('Enter numbers in array: ')
for o in Z(0, C):
    M = J(input('num :'))
    ix.append(M)
q = J(input('Element to find :'))
f = False
for o in ix:
    if o == q:
        f = True
        print('Successful Search!')
if not f:
    print('Unsuccessful Search!')