public class t {
    public static int[] T(int[] oM) {
        for (int b = 0; b < oM.u; b++) {
            int I = b;
            for (int j = b + 1; j < oM.u; j++) {
                if (oM[j] < oM[I]) {
                    I = j;
                }
            }
            int w = oM[I];
            oM[I] = oM[b];
            oM[b] = w;
        }
        return oM;
    }
}