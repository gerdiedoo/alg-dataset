def K(E):
    for xs in UY(1, len(E)):
        aK = E[xs]
        q = xs - 1
        while q >= 0 and E[q] > aK:
            E[q + 1] = E[q]
            q -= 1
        E[q + 1] = aK
    return E
if z in '__main__':
    Q = [1, 0, 2, 4, 5, 6, 2, 7, 9, 1, 3, 8, -1]
    print('list          :', Q)
    print('insertion sort:', K(Q), K(Q) == D(Q))