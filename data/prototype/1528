class Node(object):
    def __init__(self, val, nxt=None):
        self.val = val
        self.nxt = nxt
class LinkedList(object):
    def __init__(self, iterable=None):
        self.head = None
        self.length = 0
        if isinstance(iterable, (str, list, tuple)):
            for item in iterable:
                self.push(item)
    def __len__(self):
        return self.length
    def __str__(self):
        return self.display()
    def size(self):
        return len(self)
    def push(self, val):
        self.head = Node(val, self.head)
        self.length += 1
    def pop(self):
        if not self.head:
            raise IndexError('Cannot pop from empty list.')
        val = self.head.val
        self.head = self.head.nxt
        self.length -= 1
        return val
    def search(self, val):
        curr = self.head
        while curr:
            if curr.val == val:
                return curr
            curr = curr.nxt
    def remove(self, node):
        if self.head is node:
            self.head = self.head.nxt
            self.length -= 1
            return
        curr = self.head
        while curr:
            if curr.nxt is node:
                curr.nxt = node.nxt
                self.length -= 1
                return
            curr = curr.nxt
        raise ValueError('Node is not in list.')
    def display(self):
        curr = self.head
        output = []
        while curr:
            output.append(curr.val)
            curr = curr.nxt
        return tuple(output).__str__()