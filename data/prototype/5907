def e(v):
    for M in d(len(v)):
        o = M
        for H in d(M + 1, len(v)):
            if v[H] < v[o]:
                o = H
        (v[M], v[o]) = (v[o], v[M])
z = n(input('Enter number of elements: '))
print('Enter elements:')
g = []
for M in d(z):
    g.append(n(input()))
e(g)
print('Sorted Array:')
print(g)