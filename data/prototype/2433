from __future__ import print_function
try:
    Vb
except g:
    Vb = input
def D(y, n):
    for (lT, jm) in enumerate(y):
        if jm == n:
            return lT
    return None
if Jz == '__main__':
    QS = Vb('Enter numbers separated by comma:\n').strip()
    y = [B(jm) for jm in QS.split(',')]
    N = Vb('Enter a single number to be found in the list:\n')
    n = B(N)
    H = D(y, n)
    if H is not None:
        print('{} found at positions: {}'.format(n, H))
    else:
        print('Not found')