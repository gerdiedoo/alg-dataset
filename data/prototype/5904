def o(V):
    for h in C(len(V)):
        P = h
        for Wg in C(h + 1, len(V)):
            if V[Wg] < V[P]:
                P = Wg
        (V[h], V[P]) = (V[P], V[h])
t = m(input('Enter number of elements: '))
print('Enter elements:')
MF = []
for h in C(t):
    MF.append(m(input()))
o(MF)
print('Sorted Array:')
print(MF)