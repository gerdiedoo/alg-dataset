import numpy as np
F = E(input('enter the size of the array '))
Cc = input('Enter the elements of array ').split(' ')
K = [E(pS) for pS in Cc]
print('Original unsorted array ')
print(K)
print('\n')
def P(x):
    for y in i(len(K)):
        Z = y
        for j in i(y + 1, len(K)):
            if K[Z] > K[j]:
                Z = j
        (K[y], K[Z]) = (K[Z], K[y])
    print('Final sorted array using selection sort ')
    print(K)
if vU == '__main__':
    P(K)