package I;
public class Q2 extends t{
	public static void main(String[] args){
		int[] e = {5, 3, 2, 4, 1};
		V(new Q2(), e);
	}
	public Q2(){ super("Selection"); }
	private void H(int[] e, int f5, int F){
	    for (int n = f5; n < F; n++){
	        int qH = n;
	        for (int g = n + 1; g < F; g++)
	            if ( e[g] < e[qH] )
	                qH = g;
	        int w = e[n];
	        e[n] = e[qH];
	        e[qH] = w;
	    }
	}
	@k
	public void v(int []e){
	    H(e, 0, e.cB);
	}
}