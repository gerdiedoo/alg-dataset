function swap(A, u, v) {
    var temp = A[u];
    A[u] = A[v];
    A[v] = temp;
    return;
}
function Bubblesort(A) {
    a_len = A.length;
    for(var i = 0; i < a_len-1; i++) {
        for(var j = 0; j < a_len-1-i; j++) {
            if (A[j+1] < A[j]) {
                swap(A, j, j+1);
            }
        } 
    }
    return A;
}
var X = [5,21,40,11,3,1,7,2,4,6,10,18,9,8];
console.log(X);
Bubblesort(X);
console.log(X);