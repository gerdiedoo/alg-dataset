Z = [5, 2]
W = 0
O = Z[0]
v = Z[1]
A = Z[0]
Z[0] = Z[1]
Z[1] = A
print(Z)
def K(Z):
    u = len(Z) - 1
    for W in x(u, 0, -1):
        for H5 in x(W):
            if Z[H5] > Z[H5 + 1]:
                A = Z[H5]
                Z[H5] = Z[H5 + 1]
                Z[H5 + 1] = A
zq = [4, 3, 2, 1]
K(zq)
print(zq)
zq = [1, 12, 3, 4]
K(zq)
print(zq)