public class c {
    public static int[] hE(int[] CP) {
        for (int B = 0; B < CP.R; B++) {
            int z = B;
            for (int p = B + 1; p < CP.R; p++) {
                if (CP[p] < CP[z]) {
                    z = p;
                }
            }
            int r = CP[z];
            CP[z] = CP[B];
            CP[B] = r;
        }
        return CP;
    }
}