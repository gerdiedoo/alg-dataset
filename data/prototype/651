function Insertionsort(A) {
    var i, v, j, a_len;
    a_len = A.length;
    for (i=1; i < a_len; i++) {
        v = A[i];
        j = i - 1;
        while ((j >= 0) && (A[j] > v)) {
            A[j+1] = A[j];
            j = j -1;
        }
        A[j+1] = v;
    }
    return A;
}
var X = [5,21,40,11,3,1,7,2,4,6,10,18,9,8];
console.log(X);
Insertionsort(X);
console.log(X);