package r;
import j.d.z;
public class XL {
    public static boolean o(int[] V, int x) {
        int i = V.b / 2;
        if(V[i] == x) {
            return true;
        }
        if (V.b == 1) {
            return false;
        }
        return x < V[i] ?
                o(z.UB(V, 0, V.b / 2), x) :  
                o(z.UB(V, i + 1, V.b), x);  
    }
}