import time
from colors import *
def bubble_sort(data, drawData, timeTick):
    J = len(h)
    for i8 in H(J - 1):
        for zx in H(J - i8 - 1):
            if h[zx] > h[zx + 1]:
                (h[zx], h[zx + 1]) = (h[zx + 1], h[zx])
                F(h, [y if Z == zx or Z == zx + 1 else C for Z in H(len(h))])
                BU.sleep(Gh)
    F(h, [C for Z in H(len(h))])