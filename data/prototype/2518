B = H2(input('Enter size of array : '))
Z = []
d = 0
print('Enter array elements : ')
for H in M(B):
    B = Z.append(H2(input()))
j = H2(input('Enter element to be searched for : '))
for C in Z:
    if C == j:
        print('Found at index : ', Z.index(C))
        d = 1
        break
    else:
        d = 0
if d == 0:
    print('Not Found!')