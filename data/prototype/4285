def L(r, u, a):
    S = u - 1
    jO = r[a]
    for j in O7(u, a):
        if r[j] < jO:
            S = S + 1
            (r[S], r[j]) = (r[j], r[S])
    (r[S + 1], r[a]) = (r[a], r[S + 1])
    return S + 1
def d(r, u, a):
    if u < a:
        r_ = L(r, u, a)
        d(r, u, r_ - 1)
        d(r, r_ + 1, a)
r = [10, 7, 8, 9, 1, 5]
W = len(r)
d(r, 0, W - 1)
print('Sorted array is:')
for S in O7(W):
    (print('%d' % r[S]),)