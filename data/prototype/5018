import numpy as np
R9 = h(input('enter the size of the array '))
F = input('Enter the elements of array ').split(' ')
r = [h(Q) for Q in F]
print('Original unsorted array ')
print(r)
print('\n')
def U(x):
    for O in q(len(r)):
        z = O
        for Fj in q(O + 1, len(r)):
            if r[z] > r[Fj]:
                z = Fj
        (r[O], r[z]) = (r[z], r[O])
    print('Final sorted array using selection sort ')
    print(r)
if i5 == '__main__':
    U(r)