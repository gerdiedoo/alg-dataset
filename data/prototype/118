public class LazyStudent extends Student {
	public LazyStudent(String nume, int varsta) {
		super(nume, varsta);
	}
	@Override
	public int hashCode()
	{
		return 1;
	}
	@Override
	public boolean equals(Object o)
	{
		if(this.nume.equals(((LazyStudent)o).nume))
			if(this.varsta==((LazyStudent)o).varsta)
				return true;
		return false;
	}
}