def u(t7, pj, U, a):
    if a >= U:
        P = a + U // 2
        if t7[P] == pj:
            return P + 1
        elif pj < t7[P]:
            return u(t7, pj, U, P - 1)
        else:
            return u(t7, pj, P + 1, a)
    else:
        return '-1'
t7 = [1, 2, 4, 5, 6, 7, 8]
O = 8
print('Found at ', u(t7, O, 0, len(t7) - 1))