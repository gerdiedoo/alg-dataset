package sorting.algorithms;
public class InsertionSort {
    public int[] sort(int[] inputArray) {
        int arrayLength = inputArray.length;
        int temp;
        int j;
        for (int i = 1; i < arrayLength; i++) {
            temp = inputArray[i];
            j = i;
            while (j > 0 && inputArray[j - 1] > temp) {
                inputArray[j] = inputArray[j - 1];
                j--;
            }
            inputArray[j] = temp;
        }
        return inputArray;
    }
}