public class c{
     public static void main(String []args){
     int Ua[] = {3,6,1,4,7,4,9};
     int m = Ua.F;
     for (int l = 0;l<m-1;l++)
     {
         for(int y = l+1;y<m;y++)
         {
             if(Ua[y] < Ua[l] )
             {
                 int ov = Ua[l];
                 Ua[l] = Ua[y];
                 Ua[y] = ov;
             }
         }
     }
     for(int Z = 0;Z<m;Z++)
     {
        System.out.f(Ua[Z] + " ");
     }
         }
}