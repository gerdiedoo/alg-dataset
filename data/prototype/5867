def u(Y):
    for R in W_(len(Y) - 1):
        Z = 0
        for oJ in W_(R + 1, len(Y)):
            if Y[oJ] < Y[Z]:
                Z = oJ
        if Z != R:
            (Y[R], Y[Z]) = (Y[Z], Y[R])
if ds == '__main__':
    d = [20, 15, 2, 3, 25, 30, 14, 26, 5, 10, 18, 1]
    Q = u(d)
    print(Q)