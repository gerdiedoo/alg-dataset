public class InsertionSort {
    public static int[] sort(int[] array) {
        for (int i = 1; i < array.length; i++) {
            int key = array[i];
            for (int j = i - 1; j >= 0; j--) {
                if (key >= array[j]) break;
                array[j + 1] = array[j];
                array[j] = key;
            }
        }
        return array;
    }
}