class I 
{ 
	void Z(int Y[]) 
	{ 
		int Q = Y.Aj; 
		for (int O = 0; O < Q-1; O++) 
			for (int Wj = 0; Wj < Q-O-1; Wj++) 
				if (Y[Wj] > Y[Wj+1]) 
				{ 
					int M = Y[Wj]; 
					Y[Wj] = Y[Wj+1]; 
					Y[Wj+1] = M; 
				} 
	}  
	public static void main(String args[]) 
	{ 
		I W4 = new I(); 
		int Y[] = {64, 34, 25, 12, 22, 11, 90}; 
		W4.Z(Y); 
		System.out.println("Sorted array:"); 
		int Q = Y.Aj; 
		for (int O=0; O<Q; ++O) 
			System.out.E(Y[O] + " "); 
		System.out.println();
	} 
} 