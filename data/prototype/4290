def h(Y, R, O):
    k = R - 1
    f = Y[O]
    for T in V(R, O):
        if Y[T] < f:
            k = k + 1
            (Y[k], Y[T]) = (Y[T], Y[k])
    (Y[k + 1], Y[O]) = (Y[O], Y[k + 1])
    return k + 1
def X(Y, R, O):
    if R < O:
        rV = h(Y, R, O)
        X(Y, R, rV - 1)
        X(Y, rV + 1, O)
Y = [10, 7, 8, 9, 1, 5]
K = len(Y)
X(Y, 0, K - 1)
print('Sorted array is:')
for k in V(K):
    (print('%d' % Y[k]),)