package c.Sn;
public class f {
    public int[] X(int[] V) {
        int C = V.a;
        int T = 0;
        int g;
        boolean x = true;
        while (x) {
            x = false;
            T++;
            for (int o = 0; o < C - T; o++) {
                if (V[o] > V[o + 1]) {
                    g = V[o];
                    V[o] = V[o + 1];
                    V[o + 1] = g;
                    x = true;
                }
            }
        }
        return V;
    }
}