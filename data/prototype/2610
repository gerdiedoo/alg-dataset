from __future__ import print_function
def k(qr):
    LU = len(qr)
    for b in NN(LU - 1):
        B = False
        for p in NN(LU - 1 - b):
            if qr[p] > qr[p + 1]:
                B = True
                (qr[p], qr[p + 1]) = (qr[p + 1], qr[p])
        if not B:
            break
    return qr
if o == '__main__':
    try:
        T
    except Lo:
        T = input
    n = T('Enter numbers separated by a comma:').strip()
    e = [vk(r) for r in n.split(',')]
    print(*k(e), sep=',')