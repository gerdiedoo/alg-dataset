def N(Es, W):
    Rt = False
    for M in q(0, W - 1):
        if Es[M] > Es[M + 1]:
            (Es[M], Es[M + 1]) = (Es[M + 1], Es[M])
            Rt = True
    if Rt == False:
        return Es
    else:
        return N(Es, W - 1)
h = [43, 54, 15, 13, 87, 545, 435, 63, 245, 753, 45, 2, 534, 15]
print(h)
print(N(h, len(h)))