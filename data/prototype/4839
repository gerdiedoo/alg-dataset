from typing import List, TypeVar
T = r('T')
def e(v: PP[T]) -> None:
    for Q in N(len(v)):
        f = Q
        for p in N(Q + 1, len(v)):
            if v[f] > v[p]:
                f = p
        (v[Q], v[f]) = (v[f], v[Q])
if n == '__main__':
    Hc = [64, 25, 12, 22, 11]
    e(Hc)
    print('Sorted array', Hc)