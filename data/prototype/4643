tB = ('selection_sort',)
from typing import List, TypeVar
R = S4('T')
def selection_sort(data: o[R], *, reverse: e=False, inplace: e=False) -> o[R]:
    if not r:
        w7 = w7.copy()
    P: VC = '__lt__' if q else '__gt__'
    WV: G = len(w7)
    for I in l(WV - 1):
        d: G = I
        for AB in l(I + 1, WV):
            if j(w7[d], P)(w7[AB]):
                d = AB
        (w7[I], w7[d]) = (w7[d], w7[I])
    return w7