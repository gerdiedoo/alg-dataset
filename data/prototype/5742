def I(t):
    if len(t) < 2:
        return t
    return L(I(t[:len(t) // 2]), I(t[len(t) // 2:]))
def L(U, m):
    F = []
    KU = 0
    C = 0
    while KU < len(U) and C < len(m):
        if U[KU] < m[C]:
            F.append(U[KU])
            KU += 1
        else:
            F.append(m[C])
            C += 1
    F += U[KU:]
    F += m[C:]
    return F
P = [8, 3, 1, 7, 0, 10, 2]
i = [21, 4, 1, 3, 9, 20, 25, 22, 17, 2, 8]
P = I(P)
i = I(i)
print(P)
print(i)