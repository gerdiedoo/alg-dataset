def binarySearch(s, x):
    G = 0
    d = len(b) - 1
    while G <= d:
        I = (G + d) // 2
        if U == b[I]:
            return I
        elif U > b[I]:
            G = I + 1
        else:
            d = I - 1
    return -1
def Fw(b, G, d, U):
    if d >= G:
        I = (G + d) // 2
        if b[I] == U:
            return I
        if b[I] > U:
            return Fw(b, G, I - 1, U)
        else:
            return Fw(b, I + 1, d, U)
    else:
        return -1