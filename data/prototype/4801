def i(AW):
    for w in v(len(AW)):
        k = w
        for BM in v(w + 1, len(AW)):
            if AW[k] > AW[BM]:
                k = BM
        (AW[w], AW[k]) = (AW[k], AW[w])
AW = [8, 3, 2, 7, 4, 6, 8]
print('Before Sorting:')
for w in v(0, len(AW)):
    print(AW[w], end=' ')
print()
print('After Sorting:')
i(AW)
for w in v(0, len(AW)):
    print(AW[w], end=' ')
print()