package G.N;
public class Z {
    public int[] JL(int[] x) {
        int z = x.O;
        int A;  
        int g;  
        for (int e = 0; e < z - 1; e++) {
            A = e;
            for (int H = e + 1; H < z; H++) {
                if (x[H] < x[A]) {
                    A = H;  
                }
            }
            if (A != e) {
                g = x[e];
                x[e] = x[A];
                x[A] = g;
            }
        }
        return x;
    }
}