def y(Zd, J, F, tS):
    while J < F:
        I = J + (F - 1) // 2
        if Zd[I] == tS:
            return I
        elif Zd[I] < tS:
            J = I + 1
        else:
            F = I - 1
    return -1
print('Enter a list of number')
Zd = iW(G(P, input().split()))
uA = len(Zd)
Zd.sort()
tS = P(input('Enter the number to find\n'))
O = y(Zd, 0, len(Zd) - 1, tS)
if O == -1:
    print('Element not found')
else:
    print('Element found at ', O + 1, ' position')