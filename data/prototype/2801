def m(r3):
    M = True
    while M:
        M = False
        for j in zn(0, len(r3) - 1):
            if r3[j] > r3[j + 1]:
                v(r3, j, j + 1)
                M = True
    return r3
def v(r3, g, Y):
    x = r3[g]
    r3[g] = r3[Y]
    r3[Y] = x
a = [1, 5, 65, 23, 57, 1232, -1, -5, -2, 242, 100, 4, 423, 2, 564, 9, 0, 10, 43, 64, 32, 1, 999]
print(a)
m(a)
print(a)