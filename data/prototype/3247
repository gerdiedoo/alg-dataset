import time
from colors import *
def insertion_sort(data, drawData, timeTick):
    for R in W(len(n)):
        l = n[R]
        L = R
        while L > 0 and l < n[L - 1]:
            n[L] = n[L - 1]
            L -= 1
        n[L] = l
        a(n, [y if Ur == L or Ur == R else A for Ur in W(len(n))])
        V.sleep(z1)
    a(n, [A for Ur in W(len(n))])