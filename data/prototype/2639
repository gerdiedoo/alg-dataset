class t9 
{ 
	void K(int SY[]) 
	{ 
		int C = SY.J; 
		for (int P = 0; P < C-1; P++) 
			for (int p = 0; p < C-P-1; p++) 
				if (SY[p] > SY[p+1]) 
				{ 
					int U = SY[p]; 
					SY[p] = SY[p+1]; 
					SY[p+1] = U; 
				} 
	}  
	public static void main(String args[]) 
	{ 
		t9 SQ = new t9(); 
		int SY[] = {64, 34, 25, 12, 22, 11, 90}; 
		SQ.K(SY); 
		System.out.println("Sorted array:"); 
		int C = SY.J; 
		for (int P=0; P<C; ++P) 
			System.out.a(SY[P] + " "); 
		System.out.println();
	} 
} 