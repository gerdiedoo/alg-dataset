def W(w, Y, f, q):
    if q >= f:
        pG = q + f // 2
        if w[pG] == Y:
            return pG + 1
        elif Y < w[pG]:
            return W(w, Y, f, pG - 1)
        else:
            return W(w, Y, pG + 1, q)
    else:
        return '-1'
w = [1, 2, 4, 5, 6, 7, 8]
d = 8
print('Found at ', W(w, d, 0, len(w) - 1))