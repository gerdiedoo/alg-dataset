def Z(HX, h, V):
    if h < V:
        SN = u(HX, h, V)
        Z(HX, h, SN - 1)
        Z(HX, SN + 1, V)
def u(HX, h, V):
    W = HX[V]
    QR = h - 1
    for QJ in U(h, V):
        if HX[QJ] < W:
            QR += 1
            (HX[QR], HX[QJ]) = (HX[QJ], HX[QR])
    QR += 1
    (HX[QR], HX[V]) = (HX[V], HX[QR])
    return QR
if Q == '__main__':
    HX = [5, 2, 4, 6, 1, 3]
    Z(HX, 0, len(HX) - 1)
    print(HX)