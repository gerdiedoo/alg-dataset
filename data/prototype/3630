from __future__ import print_function
def f(Ng):
    def merge(wC, p):
        a = []
        while wC and p:
            a.append(wC.pop(0) if wC[0] <= p[0] else p.pop(0))
        return a + wC + p
    if len(Ng) <= 1:
        return Ng
    r = len(Ng) // 2
    return rz(f(Ng[:r]), f(Ng[r:]))
