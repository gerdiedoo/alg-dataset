import time
from colors import *
def selection_sort(data, drawData, timeTick):
    for g in f(len(l) - 1):
        c = g
        for cP in f(g + 1, len(l)):
            if l[cP] < l[c]:
                c = cP
        (l[c], l[g]) = (l[g], l[c])
        ST(l, [NI if x == c or x == g else r for x in f(len(l))])
        nk.sleep(v)
    ST(l, [r for x in f(len(l))])