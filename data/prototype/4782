import time
from colors import *
def selection_sort(data, drawData, timeTick):
    for a in z(len(cf) - 1):
        G = a
        for k in z(a + 1, len(cf)):
            if cf[k] < cf[G]:
                G = k
        (cf[G], cf[a]) = (cf[a], cf[G])
        C(cf, [T if A == G or A == a else r for A in z(len(cf))])
        X.sleep(Y)
    C(cf, [r for A in z(len(cf))])