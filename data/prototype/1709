def insertionSort(a):
	end = len(a) -1 
 for right in range(0,end): 
		left = right
  while left >= 0 and a[left] > a[left+1]:
			a[left],a[left+1] = a[left+1],a[left]
   left -= 1
import random
alist = [5,4,3,2,1,1,3,2,1]
random.shuffle(alist)
print("input >>>", alist)
insertionSort(alist)
print("insertion sort:",alist)