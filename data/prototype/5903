def U(v):
    for F in g(len(v)):
        a = F
        for j in g(F + 1, len(v)):
            if v[j] < v[a]:
                a = j
        (v[F], v[a]) = (v[a], v[F])
c9 = tB(input('Enter number of elements: '))
print('Enter elements:')
w = []
for F in g(c9):
    w.append(tB(input()))
U(w)
print('Sorted Array:')
print(w)