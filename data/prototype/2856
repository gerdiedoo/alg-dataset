import random
def z(K):
    V = 1
    while V:
        V = 0
        for Q in Rq(len(K) - 1):
            if K[Q] > K[Q + 1]:
                (K[Q], K[Q + 1]) = (K[Q + 1], K[Q])
                V += 1
vi = []
for Aw in Rq(100):
    vi.append(Aw)
l.shuffle(vi)
z(vi)
print(vi)
j = True
for Q in Rq(len(vi) - 1):
    if vi[Q] > vi[Q + 1]:
        j = False
if j:
    print('Bubble Sort successful!')