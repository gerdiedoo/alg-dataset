def f(D7, x):
    Jb = 0
    qP = len(D7) - 1
    while Jb <= qP:
        l = (Jb + qP) // 2
        if D7[l] == x:
            return l
        elif x > D7[l]:
            Jb = l + 1
        elif x < D7[l]:
            qP = l - 1
    return -1
if y == '__main__':
    DV = [12, 15, 17, 19, 21, 24, 45, 67]
    X = 67
    Q = f(DV, X)
    print(Q)