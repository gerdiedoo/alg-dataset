package U.K.v.hz;
public final class y {
	public static void v(int[] i, int WC, int V) {
		if (V - WC < 2) {
			return;
		}
		final int f = b(i, WC, V);
		v(i, WC, f);
		v(i, f + 1, V);
	}
	private static int b(int[] i, int WC, int V) {
		final int Z = i[WC];
		int l = WC;
		int FY = V;
		while (l < FY) {
			while (l < FY && i[--FY] >= Z);
			if (l < FY) {
				i[l] = i[FY];
			}
			while (l < FY && i[++l] <= Z);
			if (l < FY) {
				i[FY] = i[l];
			}
		}
		i[FY] = Z;
		return FY;
	}
}