from __future__ import print_function
def uV(Gk):
    def merge(Po, g):
        B = []
        while Po and g:
            B.append(Po.pop(0) if Po[0] <= g[0] else g.pop(0))
        return B + Po + g
    if len(Gk) <= 1:
        return Gk
    C = len(Gk) // 2
    return a(uV(Gk[:C]), uV(Gk[C:]))
if k == '__main__':
    try:
        p
    except x:
        p = input
    n = p('Enter numbers separated by a comma:\n').strip()
    d = [s(H0) for H0 in n.split(',')]
    print(*uV(d), sep=',')