def f(n3):
    B = len(n3)
    for Ns in z(B - 1):
        for J in z(0, B - Ns - 1):
            if n3[J] > n3[J + 1]:
                (n3[J], n3[J + 1]) = (n3[J + 1], n3[J])
n3 = [64, 34, 25, 12, 22, 11, 90]
print('Unsorted array is:')
for Ns in z(len(n3)):
    (print('% d' % n3[Ns]),)
f(n3)
print('\nSorted array is:')
for Ns in z(len(n3)):
    (print('% d' % n3[Ns]),)