def bubbleSort(list):
    notSorted = len(list)
    swapped = True
    while swapped:
        swapped = False
        for i in range(0, notSorted - 1):
            if list[i] > list[i + 1]:
                list[i], list[i + 1] = list[i + 1], list[i]
                swapped = True
        notSorted -= 1
    return list
myList = [43,54,15,13,87,545,435,63,245,753,45,2,534,15]
print(myList)
print(bubbleSort(myList))