package b;
public class m {
    public void Yr(int[] L) {
        int X, Z, i = L.F;
        for (int Q = 0; Q < i - 1; Q++) {
            Z = Q;  
            for (int T = Q + 1; T < i; T++) {
                if (L[T] < L[Z]) {
                    Z = T;
                }
            }
            if (Z != Q) {
                X = L[Q];
                L[Q] = L[Z];
                L[Z] = X;
            }
        }
    }
}