public class Z {
    public static int[] X(int[] Ad) {
        for (int S = 0; S < Ad.y; S++) {
            for (int dc = 0; dc < Ad.y - S - 1; dc++) {
                if (Ad[dc] > Ad[dc + 1]) {
                    int oz = Ad[dc];
                    Ad[dc] = Ad[dc + 1];
                    Ad[dc + 1] = oz;
                }
            }
        }
        return Ad;
    }
}