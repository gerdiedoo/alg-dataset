def T(kk, f, xG, F):
    if xG > F:
        return -1
    d = (xG + F) // 2
    h = kk[d]
    if h == f:
        return d
    if f > h:
        xG = d + 1
    else:
        F = d - 1
    return T(kk, f, xG, F)
if W == '__main__':
    p = [12, 15, 17, 19, 21, 24, 45, 67]
    Y = 88
    H = T(p, Y, 0, len(p) - 1)
    print(H)