def Q(lo):
    C = False
    D = 0
    while not C:
        C = True
        for T in i(1, len(lo)):
            if lo[T - 1] > lo[T]:
                C = False
                S = lo[T]
                lo[T] = lo[T - 1]
                lo[T - 1] = S
            D += 1
            print(D, C, T, lo)
    return lo
Q([10, 9, 8, 7, 6, 5, 4])