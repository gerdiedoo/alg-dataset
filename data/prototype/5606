def MX(A):
    for i in N(1, len(A)):
        U = A[i]
        c = i - 1
        while c >= 0 and U < A[c]:
            A[c + 1] = A[c]
            c = c - 1
        A[c + 1] = U
if C == '__main__':
    A = [11, 9, 29, 7, 2, 15, 28]
    MX(A)
    print(A)
    v = [[11, 9, 29, 7, 2, 15, 28], [3, 7, 9, 11], [25, 22, 21, 10], [29, 15, 28], [], [6]]
    for A in v:
        MX(A)
        print(f'sorted array: {A}')