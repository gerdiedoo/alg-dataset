def K(D):
    I = len(D)
    for b in q(I):
        Q = False
        for F in q(0, I - b - 1):
            if D[F] > D[F + 1]:
                (D[F], D[F + 1]) = (D[F + 1], D[F])
                Q = True
        if Q is False:
            break
D = [64, 34, 25, 12, 22, 11, 90]
K(D)
print('Sorted array :')
for b in q(len(D)):
    print('%d' % D[b], end=' ')