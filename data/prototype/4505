def Ry(A7, k, A):
    if k < A:
        B = H(A7, k, A)
        Ry(A7, k, B - 1)
        Ry(A7, B + 1, A)
def H(A7, k, A):
    n = A7[A]
    h = k - 1
    for E in Z(k, A):
        if A7[E] < n:
            h += 1
            (A7[h], A7[E]) = (A7[E], A7[h])
    h += 1
    (A7[h], A7[A]) = (A7[A], A7[h])
    return h
if o == '__main__':
    A7 = [5, 2, 4, 6, 1, 3]
    Ry(A7, 0, len(A7) - 1)
    print(A7)