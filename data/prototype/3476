package LL;
public class Ds {
    public static void main(String[] args) {
        int[] Qp={20,35,-15,7,55,1,-22};
        d(Qp);
    }
    public static void d(int[] Qp){
        for (int m = 1; m < Qp.B ; m++) {
            int GX = Qp[m];
            int N;
            for (N = m; N > 0 && Qp[N-1] > GX; N--) {
                Qp[N] = Qp[N-1];
            }
            Qp[N] = GX;
        }
        for(int b=0;b<Qp.B;b++){
            System.out.o(Qp[b]+" ");
        }
    }
}