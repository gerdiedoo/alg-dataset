def B(v, R6, pi):
    RI = v[pi]
    W = R6 - 1
    for p in O9(R6, pi):
        if v[p] <= RI:
            W += 1
            (v[W], v[p]) = (v[p], v[W])
    (v[W + 1], v[pi]) = (v[pi], v[W + 1])
    return (W + 1, v)
def C(v, R6=0, pi=-1):
    if pi < 0:
        pi = len(v) - 1
    if R6 < pi:
        (Xe, v) = B(v, R6, pi)
        C(v, R6, Xe - 1)
        C(v, Xe + 1, pi)
    return v