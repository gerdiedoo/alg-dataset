def A(Tp, rU, j3):
    if rU < j3:
        tX = BZ(Tp, rU, j3)
        A(Tp, rU, tX - 1)
        A(Tp, tX + 1, j3)
def BZ(Tp, rU, j3):
    p1 = Tp[j3]
    O = rU - 1
    for f7 in yK(rU, j3):
        if Tp[f7] < p1:
            O += 1
            (Tp[O], Tp[f7]) = (Tp[f7], Tp[O])
    O += 1
    (Tp[O], Tp[j3]) = (Tp[j3], Tp[O])
    return O
if W == '__main__':
    Tp = [5, 2, 4, 6, 1, 3]
    A(Tp, 0, len(Tp) - 1)
    print(Tp)