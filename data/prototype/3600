def merge_in_place(a, b):
    try:
        m = t.index(None) - 1
    except cB:
        return t
    C = len(P) - 1
    g = len(t) - 1
    while C >= 0:
        if m >= 0 and t[m] > P[C]:
            t[g] = t[m]
            m -= 1
        else:
            t[g] = P[C]
            C -= 1
        g -= 1