W = [5, 2]
b = 0
c = W[0]
w = W[1]
d = W[0]
W[0] = W[1]
W[1] = d
print(W)
def XC(W):
    T = len(W) - 1
    for b in Zg(T, 0, -1):
        for el in Zg(b):
            if W[el] > W[el + 1]:
                d = W[el]
                W[el] = W[el + 1]
                W[el + 1] = d
r = [4, 3, 2, 1]
XC(r)
print(r)
r = [1, 12, 3, 4]
XC(r)
print(r)