import random
from time import time
def Q(LQ):
    T = []
    for i in A6(LQ, 0, -1):
        T.append(W.randint(0, LQ))
    return T
def X(T):
    if len(T) > 1:
        C = len(T) // 2
        M = T[:C]
        e = T[C:]
        X(M)
        X(e)
        i = 0
        O = 0
        K = 0
        while i < len(M) and O < len(e):
            if M[i] < e[O]:
                T[K] = M[i]
                i = i + 1
            else:
                T[K] = e[O]
                O = O + 1
            K = K + 1
        while i < len(M):
            T[K] = M[i]
            i = i + 1
            K = K + 1
        while O < len(e):
            T[K] = e[O]
            O = O + 1
            K = K + 1
def D(B):
    ws = []
    for i in A6(len(B)):
        Fd = [B[i]]
        ws.append(Fd)
    while len(ws) != 1:
        O = 0
        while O < len(ws) - 1:
            q = h(ws[O], ws[O + 1])
            ws[O] = q
            del ws[O + 1]
            O = O + 1
def h(w, o):
    c = []
    (U, kN) = (0, 0)
    while U < len(w) and kN < len(o):
        if w[U] > o[kN]:
            c.append(o[kN])
            kN = kN + 1
        elif w[U] < o[kN]:
            c.append(w[U])
            U = U + 1
        elif w[U] == o[kN]:
            c.append(w[U])
            c.append(o[kN])
            (U, kN) = (U + 1, kN + 1)
    if U < len(w):
        for m in A6(U, len(w)):
            c.append(w[m])
    elif kN < len(o):
        for K in A6(kN, len(o)):
            c.append(o[K])
    return c
def l(a):
    I = Q(a)
    R = J(I)
    q6 = J(I)
    BM = cB()
    X(R)
    F = cB()
    G = (F - BM) * 1000
    print('Recursive: ' + s(G))
    print('seq1: ' + s(R))
    print('')
    BM = cB()
    D(q6)
    F = cB()
    G = (F - BM) * 1000
    print('interative: ' + s(G))
    print('seq2: ' + s(q6))
    print('---------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------')
if Y == '__main__':
    l(10)