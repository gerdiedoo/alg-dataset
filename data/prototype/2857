import random
def s(N):
    yD = 1
    while yD:
        yD = 0
        for u in n(len(N) - 1):
            if N[u] > N[u + 1]:
                (N[u], N[u + 1]) = (N[u + 1], N[u])
                yD += 1
B = []
for h0 in n(100):
    B.append(h0)
M.shuffle(B)
s(B)
print(B)
HR = True
for u in n(len(B) - 1):
    if B[u] > B[u + 1]:
        HR = False
if HR:
    print('Bubble Sort successful!')