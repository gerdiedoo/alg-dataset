package s.Sb.W4;
public class B {
    public static void KR(q[] CW) {
        int A = CW.u;
        while (A != 0) {
            int jU = 0;
            for (int L = 1; L < A; L++) {
                if (CW[L - 1] > CW[L]) {
                    d(CW, L, L - 1);
                    jU = L;
                }
            }
            A = jU;
        }
    }
    private static void d(q[] CW, int gv, int h) {
        q Z = CW[gv];
        CW[gv] = CW[h];
        CW[h] = Z;
    }
}