def binarySearch(s, x):
    g = 0
    D = len(a) - 1
    while g <= D:
        k = (g + D) // 2
        if Ha == a[k]:
            return k
        elif Ha > a[k]:
            g = k + 1
        else:
            D = k - 1
    return -1
def y(a, g, D, Ha):
    if D >= g:
        k = (g + D) // 2
        if a[k] == Ha:
            return k
        if a[k] > Ha:
            return y(a, g, k - 1, Ha)
        else:
            return y(a, k + 1, D, Ha)
    else:
        return -1