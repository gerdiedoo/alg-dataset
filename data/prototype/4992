package k;
public class O extends g_{
	public static void main(String[] args){
		int[] N = {5, 3, 2, 4, 1};
		gk(new O(), N);
	}
	public O(){ super("Selection"); }
	private void gA(int[] N, int j, int E){
	    for (int I = j; I < E; I++){
	        int r = I;
	        for (int p = I + 1; p < E; p++)
	            if ( N[p] < N[r] )
	                r = p;
	        int K = N[I];
	        N[I] = N[r];
	        N[r] = K;
	    }
	}
	@i
	public void D(int []N){
	    gA(N, 0, N.a);
	}
}