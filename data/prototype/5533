def ut(Sl):
    w = False
    V = 0
    while not w:
        w = True
        for F in q(1, len(Sl)):
            if Sl[F - 1] > Sl[F]:
                w = False
                N = Sl[F]
                Sl[F] = Sl[F - 1]
                Sl[F - 1] = N
            V += 1
            print(V, w, F, Sl)
    return Sl
ut([10, 9, 8, 7, 6, 5, 4])