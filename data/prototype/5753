def l(Y):
    if len(Y) <= 1:
        return Y
    e = len(Y) // 2
    O = l(Y[:e])
    u = l(Y[e:])
    return a(O, u)
def a(O, u):
    Y = []
    (C, J) = (0, 0)
    while C < len(O) and J < len(u):
        if O[C] <= u[J]:
            Y.append(O[C])
            C += 1
        else:
            Y.append(u[J])
            J += 1
    Y += O[C:]
    Y += u[J:]
    return Y
print(a([1, 2, 3], [0, 4, 6, 8]))
print(a([1, 2, 4], [0, 1, 3]))
print(l([2, 0, 1, 4, 3]))