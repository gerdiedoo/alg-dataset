def IE(V, w, dir, o):
    while w <= dir:
        while V[w] < o:
            w += 1
        while V[dir] > o:
            dir -= 1
        if w <= dir:
            (V[w], V[dir]) = (V[dir], V[w])
            w += 1
            dir -= 1
    return w
def s(V, w, dir):
    if w >= dir:
        return
    o = V[(w + dir) / 2]
    W3 = IE(V, w, dir, o)
    s(V, w, W3 - 1)
    s(V, W3, dir)