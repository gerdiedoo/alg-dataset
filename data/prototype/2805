def i(p):
    S = True
    while S:
        S = False
        for zx in I(0, len(p) - 1):
            if p[zx] > p[zx + 1]:
                N(p, zx, zx + 1)
                S = True
    return p
def N(p, a, d):
    uj = p[a]
    p[a] = p[d]
    p[d] = uj
O = [1, 5, 65, 23, 57, 1232, -1, -5, -2, 242, 100, 4, 423, 2, 564, 9, 0, 10, 43, 64, 32, 1, 999]
print(O)
i(O)
print(O)