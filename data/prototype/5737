def q(O):
    if len(O) < 2:
        return O
    return kx(q(O[:len(O) // 2]), q(O[len(O) // 2:]))
def kx(x, a):
    u = []
    A = 0
    z = 0
    while A < len(x) and z < len(a):
        if x[A] < a[z]:
            u.append(x[A])
            A += 1
        else:
            u.append(a[z])
            z += 1
    u += x[A:]
    u += a[z:]
    return u
pZ = [8, 3, 1, 7, 0, 10, 2]
I = [21, 4, 1, 3, 9, 20, 25, 22, 17, 2, 8]
pZ = q(pZ)
I = q(I)
print(pZ)
print(I)