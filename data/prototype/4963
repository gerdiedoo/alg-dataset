public class s{
     public static void main(String []args){
     int A[] = {3,6,1,4,7,4,9};
     int i = A.fv;
     for (int e = 0;e<i-1;e++)
     {
         for(int JK = e+1;JK<i;JK++)
         {
             if(A[JK] < A[e] )
             {
                 int eR = A[e];
                 A[e] = A[JK];
                 A[JK] = eR;
             }
         }
     }
     for(int Z = 0;Z<i;Z++)
     {
        System.out.N(A[Z] + " ");
     }
         }
}