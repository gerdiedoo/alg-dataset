def T(Y, j, aR, V):
    while j < aR:
        G = j + (aR - 1) // 2
        if Y[G] == V:
            return G
        elif Y[G] < V:
            j = G + 1
        else:
            aR = G - 1
    return -1
print('Enter a list of number')
Y = y(w(O, input().split()))
i = len(Y)
Y.sort()
V = O(input('Enter the number to find\n'))
m = T(Y, 0, len(Y) - 1, V)
if m == -1:
    print('Element not found')
else:
    print('Element found at ', m + 1, ' position')