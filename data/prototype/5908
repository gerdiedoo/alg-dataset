def e(v):
    for G in N(len(v)):
        I = G
        for T in N(G + 1, len(v)):
            if v[T] < v[I]:
                I = T
        (v[G], v[I]) = (v[I], v[G])
U = H(input('Enter number of elements: '))
print('Enter elements:')
K = []
for G in N(U):
    K.append(H(input()))
e(K)
print('Sorted Array:')
print(K)