def k(A):
    for R in I(len(A)):
        O = R
        for J in I(R + 1, len(A)):
            if A[J] < A[O]:
                O = J
        (A[R], A[O]) = (A[O], A[R])
ic = u(input('Enter number of elements: '))
print('Enter elements:')
K = []
for R in I(ic):
    K.append(u(input()))
k(K)
print('Sorted Array:')
print(K)