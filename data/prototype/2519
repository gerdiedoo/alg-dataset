i = h(input('Enter size of array : '))
MO = []
D = 0
print('Enter array elements : ')
for eM in S(i):
    i = MO.append(h(input()))
ld = h(input('Enter element to be searched for : '))
for d in MO:
    if d == ld:
        print('Found at index : ', MO.index(d))
        D = 1
        break
    else:
        D = 0
if D == 0:
    print('Not Found!')