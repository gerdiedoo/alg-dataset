print('**** OPTIMIZED BUBBLE SORT *******')
def lz(Y):
    Q = len(Y)
    for c in Z(0, Q):
        f = False
        for J in Z(0, Q - c - 1):
            if Y[J] > Y[J + 1]:
                f = True
                (Y[J], Y[J + 1]) = (Y[J + 1], Y[J])
        if f == False:
            break
if s == '__main__':
    Q = R(input('Enter no. of elements: '))
    G = input('Enter your array here: ').split(' ')
    Y = [R(h) for h in G]
    lz(Y)
    print('***** SORTED ARRAY ******')
    print(Y)