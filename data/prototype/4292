def R(V, T, TU):
    P = T - 1
    z = V[TU]
    for Xu in N(T, TU):
        if V[Xu] < z:
            P = P + 1
            (V[P], V[Xu]) = (V[Xu], V[P])
    (V[P + 1], V[TU]) = (V[TU], V[P + 1])
    return P + 1
def mZ(V, T, TU):
    if T < TU:
        Yx = R(V, T, TU)
        mZ(V, T, Yx - 1)
        mZ(V, Yx + 1, TU)
V = [10, 7, 8, 9, 1, 5]
M = len(V)
mZ(V, 0, M - 1)
print('Sorted array is:')
for P in N(M):
    (print('%d' % V[P]),)