import time
from colors import *
def insertion_sort(data, drawData, timeTick):
    for u in W(len(Q)):
        C = Q[u]
        L = u
        while L > 0 and C < Q[L - 1]:
            Q[L] = Q[L - 1]
            L -= 1
        Q[L] = C
        E(Q, [j if k == L or k == u else G for k in W(len(Q))])
        z.sleep(x)
    E(Q, [G for k in W(len(Q))])