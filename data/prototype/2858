import random
def sW(x):
    Y = 1
    while Y:
        Y = 0
        for iw in g(len(x) - 1):
            if x[iw] > x[iw + 1]:
                (x[iw], x[iw + 1]) = (x[iw + 1], x[iw])
                Y += 1
hj = []
for m in g(100):
    hj.append(m)
v.shuffle(hj)
sW(hj)
print(hj)
t1 = True
for iw in g(len(hj) - 1):
    if hj[iw] > hj[iw + 1]:
        t1 = False
if t1:
    print('Bubble Sort successful!')