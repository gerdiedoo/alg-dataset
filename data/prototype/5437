def x(Ne, w):
    c = 0
    X = len(Ne) - 1
    while c <= X:
        mm = (c + X) // 2
        if Ne[mm] == w:
            return mm
        elif w > Ne[mm]:
            c = mm + 1
        elif w < Ne[mm]:
            X = mm - 1
    return -1
if S == '__main__':
    j = [12, 15, 17, 19, 21, 24, 45, 67]
    XI = 67
    W = x(j, XI)
    print(W)