package n5.Tw.L;
public class AD {
    public static void Mb(q[] k) {
        for (int H = 0; H < k.c - 1; H++) {
            int v = H;
            for (int p = H + 1; p < k.c; p++) {
                if (k[p] < k[v]) {
                    v = p;
                }
            }
            if (v != H) {
                X(k, H, v);
            }
        }
    }
    private static void X(q[] k, int Q, int a) {
        q K = k[Q];
        k[Q] = k[a];
        k[a] = K;
    }
}