def gx(i):
    if len(i) < 2:
        return i
    return s(gx(i[:len(i) // 2]), gx(i[len(i) // 2:]))
def s(x, So):
    J = []
    RY = 0
    eU = 0
    while RY < len(x) and eU < len(So):
        if x[RY] < So[eU]:
            J.append(x[RY])
            RY += 1
        else:
            J.append(So[eU])
            eU += 1
    J += x[RY:]
    J += So[eU:]
    return J
sq = [8, 3, 1, 7, 0, 10, 2]
N = [21, 4, 1, 3, 9, 20, 25, 22, 17, 2, 8]
sq = gx(sq)
N = gx(N)
print(sq)
print(N)