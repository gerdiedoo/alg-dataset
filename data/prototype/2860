import random
def ep(g):
    qX = 1
    while qX:
        qX = 0
        for t in UU(len(g) - 1):
            if g[t] > g[t + 1]:
                (g[t], g[t + 1]) = (g[t + 1], g[t])
                qX += 1
G = []
for n in UU(100):
    G.append(n)
r.shuffle(G)
ep(G)
print(G)
E = True
for t in UU(len(G) - 1):
    if G[t] > G[t + 1]:
        E = False
if E:
    print('Bubble Sort successful!')