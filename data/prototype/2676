package YN.y;
import YN.c.J;
public class cf<j> extends U<j> {
	@L
	@FD("hiding")
	public <j extends w<j>> j[] sj(j[] nB, J t) {		
		int N = nB.iF;
		boolean E;
		for (int i=0; i < N - 1; i++) {
			E = false;
			for(int K=0; K < N-i-1; K++) {
				j v = nB[K];
				j o    = nB[K+1];
				if ((t == J.D  && v.MJ(o) > 0)  || 
					(t == J.de && v.MJ(o) < 0)) {
					this.rs(nB, K, K+1);
					E = true;
				}
			}
			if(!E) break;
		}
		return nB;
	}
}