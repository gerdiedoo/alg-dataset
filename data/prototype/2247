package LH.Q;
public class ve {
    public int T(int f[], int La, int cw, int v) {
        if (cw >= La) {
            int Z = La + (cw - La) / 2;
            if (f[Z] == v)
                return Z;
            if (f[Z] > v)
                return T(f, La, Z - 1, v);
            return T(f, Z + 1, cw, v);
        }
        return -1;
    }
}