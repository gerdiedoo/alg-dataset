class  bB
{ 
	public static int a(int v[], int m2) 
	{ 
		int N = v.E; 
		for(int Z = 0; Z < N; Z++) 
		{ 
			if(v[Z] == m2) 
				return Z; 
		} 
		return -1; 
	} 
	public static void main(String args[]) 
	{ 
		int v[] = { 2, 3, 4, 10, 40 }; 
		int m2 = 10; 
		int O = a(v, m2); 
		if(O == -1) 
			System.out.Q("Element is not present in array"); 
		else
			System.out.Q("Element is present at index " + O); 
	} 
} 