package com.deepak.algorithms.Searching;
public class LinearSearch {
	public static int performLinearSearch(int[] listOfValues, int targetValue) {
		int index = -1;
		for (int i=0; i < listOfValues.length; i++) {
			if (listOfValues[i] == targetValue) {
				index = i;
				break;
			}
		}
		return index;
	}
}