public class KP {
    public static void l(int[] e) {
        int Kb = e.L;
        for (int G = 0; Kb != 1 && G < e.L - 1; G++) {
            int y = Kb;
            for (int bm = 0; bm < y - 1; bm++) {
                if (e[bm] > e[bm + 1]) {
                    lj(e, bm, bm + 1);
                    Kb = bm + 1;
                }
            }
        }
    }
    private static void lj(int[] e, int S, int o) {
        int y = e[S];
        e[S] = e[o];
        e[o] = y;
    }
}