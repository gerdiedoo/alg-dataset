def selectionSort(a):
    for i in range(len(a)):
        current_min = i
        for j in range(i+1,len(a)):
            if a[current_min] > a[j]:
                current_min = j
        a[i], a[current_min] = a[current_min], a[i]
a = [8, 3, 2, 7, 4, 6, 8]
print("Before Sorting:")
for i in range(0, len(a)):
    print(a[i], end = ' ')
print()
print("After Sorting:") 
selectionSort(a)
for i in range(0, len(a)):
    print(a[i], end = ' ')
print()