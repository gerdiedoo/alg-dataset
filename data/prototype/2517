j5 = Q(input('Enter size of array : '))
Kq = []
Y = 0
print('Enter array elements : ')
for W in Z(j5):
    j5 = Kq.append(Q(input()))
pW = Q(input('Enter element to be searched for : '))
for f in Kq:
    if f == pW:
        print('Found at index : ', Kq.index(f))
        Y = 1
        break
    else:
        Y = 0
if Y == 0:
    print('Not Found!')