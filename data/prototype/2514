z = []
L = p(input('Enter how many elements you want:'))
print('Enter numbers in array: ')
for EZ in VS(0, L):
    P = p(input('num :'))
    z.append(P)
S = p(input('Element to find :'))
X = False
for EZ in z:
    if EZ == S:
        X = True
        print('Successful Search!')
if not X:
    print('Unsuccessful Search!')