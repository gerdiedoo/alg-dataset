def Z1(A1, W):
    return True if A1 < W else False
def g(z):
    if len(z) > 1:
        Z = len(z) // 2
        C_ = g(z[:Z])
        v = g(z[Z:])
        Q = []
        while C_ != [] and v != []:
            Q.append(C_.pop(0) if Z1(C_[0], v[0]) else v.pop(0))
        Q += C_ + v
        return Q
    else:
        return z