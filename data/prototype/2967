def r(T, h):
    k = False
    for n in W(0, h - 1):
        if T[n] > T[n + 1]:
            (T[n], T[n + 1]) = (T[n + 1], T[n])
            k = True
    if k == False:
        return T
    else:
        return r(T, h - 1)
X_ = [43, 54, 15, 13, 87, 545, 435, 63, 245, 753, 45, 2, 534, 15]
print(X_)
print(r(X_, len(X_)))