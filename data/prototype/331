const binarySearch = (arr, k) => {
  let min = 0;
  let max = arr.length - 1;
  while (min <= max) {
    const cur = Math.floor((min + max) / 2);
    if (arr[cur] === k) {
      return cur;
    }
    (arr[cur] > k) ?
      max = cur - 1 :
      min = cur + 1;
  }
  return -1;
};
module.exports = binarySearch;