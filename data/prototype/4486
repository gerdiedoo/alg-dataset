def h(D, E, Gw):
    Z = D[Gw]
    U = E - 1
    for s in r(E, Gw):
        if D[s] < Z:
            (D[s], D[U]) = (D[U], D[s])
            U += 1
    (D[Gw], D[U + 1]) = (D[U + 1], D[Gw])
    return U + 1
def x(D, E, Gw):
    if E < Gw:
        C = h(D, E, Gw)
        x(D, E, C - 1)
        x(D, C + 1, E)
def m():
    K = [10, 2, 5, 8, 1]
    x(K, 0, len(K) - 1)
    print(K)
    print('Hello World!')
if M == '__main__':
    m()