class SelectionSort:
    def sort(A):
        def swap(A, u, v):
            temp = A[u]
            A[u] = A[v]
            A[v] = temp
        a_len = len(A)
        for i in range(0, a_len - 1):
            curr_min = i
            for j in range(i+1, a_len):
                if(A[j] < A[curr_min]):
                   curr_min = j
            swap(A, i, curr_min)
        return A