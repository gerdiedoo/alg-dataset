def T(v, Q9, y, K):
    while Q9 < y:
        d = Q9 + (y - 1) // 2
        if v[d] == K:
            return d
        elif v[d] < K:
            Q9 = d + 1
        else:
            y = d - 1
    return -1
print('Enter a list of number')
v = u(Rd(Q, input().split()))
vK = len(v)
v.sort()
K = Q(input('Enter the number to find\n'))
L = T(v, 0, len(v) - 1, K)
if L == -1:
    print('Element not found')
else:
    print('Element found at ', L + 1, ' position')