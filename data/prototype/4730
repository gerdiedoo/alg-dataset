def u(g, z, b):
    I = z
    G = z
    while G <= b:
        if g[G] > g[I]:
            I = G
        G = G + 1
    return I
def rw(g):
    for G in AP(len(g) - 1, 0, -1):
        I = u(g, 0, G)
        if I != G:
            X = g[G]
            g[G] = g[I]
            g[I] = X
    print(G, g)
if k == '__main__':
    J = [5, 10, 58, 23, 1, 2]
    rw(J)