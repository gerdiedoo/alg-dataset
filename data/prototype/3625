from __future__ import print_function
def pA(K):
    def merge(Fb, g):
        O = []
        while Fb and g:
            O.append(Fb.pop(0) if Fb[0] <= g[0] else g.pop(0))
        return O + Fb + g
    if len(K) <= 1:
        return K
    G = len(K) // 2
    return m(pA(K[:G]), pA(K[G:]))
if k == '__main__':
    try:
        J
    except Z8:
        J = input
    o = J('Enter numbers separated by a comma:\n').strip()
    f = [iq(D) for D in o.split(',')]
    print(*pA(f), sep=',')