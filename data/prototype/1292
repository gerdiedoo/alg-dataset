package io.github.marioluan.algorithms.sorting;
public final class SelectionSort extends Sortable {
    private SelectionSort() {
    }
    @SuppressWarnings("rawtypes")
    public static void sort(Comparable[] a) {
        int n = a.length;
        for (int i = 0; i < n; i++) {
            int min = findMin(a, i, n);
            swap(a, i, min);
        }
    }
}