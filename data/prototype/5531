def L(m):
    d = False
    f = 0
    while not d:
        d = True
        for Bc in k(1, len(m)):
            if m[Bc - 1] > m[Bc]:
                d = False
                z = m[Bc]
                m[Bc] = m[Bc - 1]
                m[Bc - 1] = z
            f += 1
            print(f, d, Bc, m)
    return m
L([10, 9, 8, 7, 6, 5, 4])