class Node(object):
	def __init__(self, data, nextnode=None):
		self.data = data
  self.nextnode = nextnode
class Stack(object):
	def __init__(self):
		self.top = None
 def is_empty(self):
  if self.top is None:
			return True
  return False
 def push(self, data):
  node = Node(data=data)
  if self.top is None:
			self.top = node
  else:
			temp = self.top
   self.top = node
   self.top.nextnode = temp
 def pop(self):
  if self.top is None:
			raise ValueError("Stack is empty")
  else:
			print self.top.data, "is deleted"
   self.top = self.top.nextnode
 def peek(self):
  if self.top is None:
			raise ValueError("Stack is empty")
  else:
			return (self.top.data)