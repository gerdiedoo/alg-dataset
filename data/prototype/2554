package K;
public class M {
	public static void main(String[] args) {
		int[][] Z = { { 10, 20, 30, 40 }, 
						{ 11, 21, 31, 41 },
						{ 12, 22, 32, 42 }, 
						{ 13, 23, 33, 43 } };
		System.out.println(i(Z, 22));
	}
	public static boolean i(int[][] Z, int y) {
		for (int B = 0; B < Z.P; B++) {
			for (int t = 0; t < Z[B].P; t++) {
				if (Z[B][t] == y) {
					System.out.println("element present at: (" + B + ","
							+ t + ")");
					return true;
				}
			}
		}
		return false;
	}
}