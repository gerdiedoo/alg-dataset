def a(X, R, H):
    c = R - 1
    z = X[H]
    for ry in N(R, H):
        if X[ry] < z:
            c = c + 1
            (X[c], X[ry]) = (X[ry], X[c])
    (X[c + 1], X[H]) = (X[H], X[c + 1])
    return c + 1
def m0(X, R, H):
    if R < H:
        JI = a(X, R, H)
        m0(X, R, JI - 1)
        m0(X, JI + 1, H)
X = [10, 7, 8, 9, 1, 5]
M = len(X)
m0(X, 0, M - 1)
print('Sorted array is:')
for c in N(M):
    (print('%d' % X[c]),)