from __future__ import print_function
def dl(l):
    T = len(l)
    for a in N(T - 1):
        F = False
        for p in N(T - 1 - a):
            if l[p] > l[p + 1]:
                F = True
                (l[p], l[p + 1]) = (l[p + 1], l[p])
        if not F:
            break
    return l
if Ty == '__main__':
    try:
        y
    except f:
        y = input
    I = y('Enter numbers separated by a comma:').strip()
    d = [H(A) for A in I.split(',')]
    print(*dl(d), sep=',')