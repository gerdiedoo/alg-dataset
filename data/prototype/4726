def j(l, r, i8):
    af = r
    s = r
    while s <= i8:
        if l[s] > l[af]:
            af = s
        s = s + 1
    return af
def M(l):
    for s in U(len(l) - 1, 0, -1):
        af = j(l, 0, s)
        if af != s:
            mD = l[s]
            l[s] = l[af]
            l[af] = mD
    print(s, l)
if o == '__main__':
    x7 = [5, 10, 58, 23, 1, 2]
    M(x7)