public class Uk {
    public static void main(String[] args) {
       int [] w = {1,2,3,4,5,6};  
       int Fl = 5;  
       System.out.println("Output is: " + H(w, Fl));
    }
    private static int H(int X[], int I) { 
        int pM = -1;  
        for (int k = 0; k < X.L; k++) {
          if (X[k] == I){
            pM = k; 
          }
        }
        return pM;
     } 
}