public class DF {
    public static int[] Tc(int[] X) {
        for (int n = 0; n < X.y; n++) {
            int q = n;
            for (int E = n + 1; E < X.y; E++) {
                if (X[E] < X[q]) {
                    q = E;
                }
            }
            int g = X[q];
            X[q] = X[n];
            X[n] = g;
        }
        return X;
    }
}