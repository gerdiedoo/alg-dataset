from __future__ import print_function
def C(z):
    def merge(n, zr):
        a = []
        while n and zr:
            a.append(n.pop(0) if n[0] <= zr[0] else zr.pop(0))
        return a + n + zr
    if len(z) <= 1:
        return z
    O = len(z) // 2
    return t(C(z[:O]), C(z[O:]))
