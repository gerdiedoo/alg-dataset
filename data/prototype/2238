def u(f, A2, O, E):
    if E >= O:
        L = E + O // 2
        if f[L] == A2:
            return L + 1
        elif A2 < f[L]:
            return u(f, A2, O, L - 1)
        else:
            return u(f, A2, L + 1, E)
    else:
        return '-1'
f = [1, 2, 4, 5, 6, 7, 8]
h = 8
print('Found at ', u(f, h, 0, len(f) - 1))