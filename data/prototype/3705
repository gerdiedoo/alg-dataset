def r(u):
    if len(u) <= 1:
        return u
    G = len(u) // 2
    K = r(u[:G])
    A = r(u[G:])
    Sw = []
    j8 = 0
    R = 0
    while j8 < len(K) and R < len(A):
        if K[j8] < A[R]:
            Sw.append(K[j8])
            j8 += 1
        else:
            Sw.append(A[R])
            R += 1
    Sw += K[j8:]
    Sw += A[R:]
    return Sw
X = [3, 21, 7, 1, 9, 17, 14, 2, 5, 10, 6]
print(r(X))