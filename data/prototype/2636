class f 
{ 
	void A(int X[]) 
	{ 
		int C = X.m; 
		for (int G = 0; G < C-1; G++) 
			for (int B7 = 0; B7 < C-G-1; B7++) 
				if (X[B7] > X[B7+1]) 
				{ 
					int vP = X[B7]; 
					X[B7] = X[B7+1]; 
					X[B7+1] = vP; 
				} 
	}  
	public static void main(String args[]) 
	{ 
		f P = new f(); 
		int X[] = {64, 34, 25, 12, 22, 11, 90}; 
		P.A(X); 
		System.out.println("Sorted array:"); 
		int C = X.m; 
		for (int G=0; G<C; ++G) 
			System.out.OB(X[G] + " "); 
		System.out.println();
	} 
} 