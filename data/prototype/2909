package W;
public class N extends KS{
	public static void main(String[] args){
		int[] C = {5, 3, 2, 4, 1};
		r(new N(), C);
	}
	public N(){ super("Bubble"); }
	private void g(int[] C, int p, int zK){
		for (int P = p; P < zK; P++)
			for (int T = p; T < zK - P - 1; T++)
			{	 
				if ( C[T] > C[T+1] )
				{
					int M = C[T];
					C[T] =  C[T+1];
					C[T+1] = M;
				}
			}
	}
	@h
	public void x(int[] C){
		g(C, 0, C.O);
	}
}