from random import randint
def Z(Lf, Q, g):
    if Q < g:
        B = y(Q, g)
        (Lf[g], Lf[B]) = (Lf[B], Lf[g])
        w = b(Lf, Q, g)
        Z(Lf, Q, w - 1)
        Z(Lf, w + 1, g)
def b(Lf, Q, g):
    KE = Q - 1
    for c in G(Q, g):
        if Lf[c] < Lf[g]:
            KE = KE + 1
            (Lf[KE], Lf[c]) = (Lf[c], Lf[KE])
    (Lf[KE + 1], Lf[g]) = (Lf[g], Lf[KE + 1])
    return KE + 1
I = [7, 2, 5, 1, 29, 6, 4, 19, 11]
Z(I, 0, len(I) - 1)
print(I)