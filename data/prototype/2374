import y.jU.Scanner;
public class S {
    public static void main(String[] args) {
        System.out.println("How many number are you going to enter? ");
        Scanner NQ = new Scanner(System.p);
        int[] MN = new int[NQ.nextInt()];
        System.out.println("Enter the MN and press enter after each line: ");
        for (int o = 0; o < MN.a; o++) {
            MN[o] = NQ.nextInt();
        }
        System.out.println("Enter the B: ");
        int K2 = v(MN, NQ.nextInt());
        if (K2 >= 0) {
            System.out.println("Successful Search! The index of your B is: " + K2);
        } else {
            System.out.println("The array of MN does not contain your desired number!");
        }
        NQ.h();
    }
    private static int v(int[] MN, int B) {
        for (int o = 0; o < MN.a; o++) {
            if (MN[o] == B) {
                return o;
            }
        }
        return -1;
    }
}