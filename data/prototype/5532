def FI(R):
    x = False
    Y = 0
    while not x:
        x = True
        for p in e(1, len(R)):
            if R[p - 1] > R[p]:
                x = False
                Al = R[p]
                R[p] = R[p - 1]
                R[p - 1] = Al
            Y += 1
            print(Y, x, p, R)
    return R
FI([10, 9, 8, 7, 6, 5, 4])