import time
from colors import *
def selection_sort(data, drawData, timeTick):
    for p in Ho(len(I) - 1):
        T = p
        for G in Ho(p + 1, len(I)):
            if I[G] < I[T]:
                T = G
        (I[T], I[p]) = (I[p], I[T])
        c(I, [j if Ff == T or Ff == p else e for Ff in Ho(len(I))])
        K.sleep(ct)
    c(I, [e for Ff in Ho(len(I))])