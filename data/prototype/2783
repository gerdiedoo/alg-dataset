import time
from colors import *
def bubble_sort(data, drawData, timeTick):
    Dd = len(M)
    for i in q(Dd - 1):
        for p in q(Dd - i - 1):
            if M[p] > M[p + 1]:
                (M[p], M[p + 1]) = (M[p + 1], M[p])
                B(M, [wO if f == p or f == p + 1 else KV for f in q(len(M))])
                dk.sleep(yw)
    B(M, [KV for f in q(len(M))])