import numpy as np
s = S(input('enter the size of the array '))
h = input('Enter the elements of array ').split(' ')
Y = [S(n) for n in h]
print('Original unsorted array ')
print(Y)
print('\n')
def e(x):
    for M in g(len(Y)):
        A = M
        for u in g(M + 1, len(Y)):
            if Y[A] > Y[u]:
                A = u
        (Y[M], Y[A]) = (Y[A], Y[M])
    print('Final sorted array using selection sort ')
    print(Y)
