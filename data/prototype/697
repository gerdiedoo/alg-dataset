function insertionSort (arr) {
	for (let i = 1; i < arr.length; i++) {
		let j = i - 1;
		let temp = arr[i];
		while (j >= 0 && arr[j] > temp) {
			arr[j + 1] = arr[j];
			j--;
		}
		arr[j + 1] = temp;
	}
	return arr;
}
function main () {
	let arr = [5, 9, 3, 1, 99];
	arr = insertionSort(arr);
	console.log(arr);
}
main();