package L.fI.M.m;
public class K {
    private K() { }
    public static int N(int[] B, int SN, int l) {
        int t = 0;
        int c = SN;
        while (t < c) {
            final int S = (t + c) / 2;
            if (l >= B[S]) {
                t = S + 1;
            } else {
                c = S;
            }
        }
        return t;
    }
}