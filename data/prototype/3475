package l;
public class q {
    public static void main(String[] args) {
        int[] w={20,35,-15,7,55,1,-22};
        pj(w);
    }
    public static void pj(int[] w){
        for (int T = 1; T < w.i ; T++) {
            int e = w[T];
            int G;
            for (G = T; G > 0 && w[G-1] > e; G--) {
                w[G] = w[G-1];
            }
            w[G] = e;
        }
        for(int H=0;H<w.i;H++){
            System.out.LC(w[H]+" ");
        }
    }
}