def u(N):
    for D1 in WP(len(N)):
        X = D1
        for h in WP(D1, len(N)):
            if N[h] < N[X]:
                X = h
        (N[D1], N[X]) = (N[X], N[D1])
if dw == '__main__':
    print('Enter sequence(separated by spaces): ')
    d = [E(K) for K in input().split()]
    u(d)
    print(d)