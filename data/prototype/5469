def binarySearch(s, x):
    x = 0
    zo = len(U) - 1
    while x <= zo:
        r = (x + zo) // 2
        if A == U[r]:
            return r
        elif A > U[r]:
            x = r + 1
        else:
            zo = r - 1
    return -1
def c(U, x, zo, A):
    if zo >= x:
        r = (x + zo) // 2
        if U[r] == A:
            return r
        if U[r] > A:
            return c(U, x, r - 1, A)
        else:
            return c(U, r + 1, zo, A)
    else:
        return -1