import time
from colors import *
def bubble_sort(data, drawData, timeTick):
    Mt = len(G)
    for ND in N(Mt - 1):
        for p in N(Mt - ND - 1):
            if G[p] > G[p + 1]:
                (G[p], G[p + 1]) = (G[p + 1], G[p])
                B(G, [O if w == p or w == p + 1 else K for w in N(len(G))])
                X.sleep(v)
    B(G, [K for w in N(len(G))])