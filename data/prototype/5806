def r(A, j, J):
    Z = A[j]
    A[j] = A[J]
    A[J] = Z
def p(A, m, f):
    Ri = A[f]
    u = m
    for a in q(m, f):
        if A[a] <= Ri:
            r(A, a, u)
            u = u + 1
    r(A, f, u)
    return u
def rP(A, m, f):
    if m < f:
        u = p(A, m, f)
        rP(A, u + 1, f)
        rP(A, m, u - 1)
A = [21, 4, 1, 3, 9, 20, 25, 6, 21, 14]
rP(A, 0, 9)
print(A)