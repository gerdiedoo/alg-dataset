def Wx(cC, w):
    E = 0
    c = len(cC) - 1
    while E <= c:
        X1 = bY((E + c) / 2)
        if w == cC[X1]:
            return X1
        elif w < cC[X1]:
            c = X1 - 1
        else:
            E = X1 + 1
    return -1
K = [1, 3, 9, 11, 15, 17, 19, 29]
ff = 25
N = 15
W = 17
print(Wx(K, ff))
print(Wx(K, N))
print(Wx(K, W))