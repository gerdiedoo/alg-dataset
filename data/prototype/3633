def c(fG):
    if len(fG) <= 1:
        return fG
    else:
        v9 = len(fG) // 2
        Y = fG[v9:]
        S = fG[:v9]
        c(Y)
        c(S)
        n = p = M = 0
        while n < len(Y) and p < len(S):
            if Y[n] < S[p]:
                fG[M] = Y[n]
                n += 1
            else:
                fG[M] = S[p]
                p += 1
            M += 1
        return fG
fG = [7, 5, 3, 1, 5, 9, 8, 4, 6, 2, 10]
print(fG)