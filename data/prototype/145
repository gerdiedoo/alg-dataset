public class MapEntry<K,V> implements Entry<K,V>{
	private K key;
	private V value;
	private int count;
	public MapEntry(K key, V value) {
		this.key = key;
		this.value = value;
		this.count = 1;
	}
	@Override
	public int getCount() {
		return this.count;
	}
	@Override
	public K getKey() {
		return this.key;
	}
	@Override
	public V getValue() {
		return this.value;
	}
	protected void setKey(K key) {
		this.key = key;
	}
	protected V setValue(V value) {
		V old = this.value;
		this.value = value;
		return old;
	}
	protected void setCount(int count) {
		this.count = count;
	}
	protected void increaseCount() {
		this.count++;
	}
	@Override
	public int hashCode() {
		return this.key.hashCode();
	}
}