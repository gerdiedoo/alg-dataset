def K(v, J, H):
    o = v[J]
    v[J] = v[H]
    v[H] = o
def uT(v, s, X):
    W = v[X]
    m = s
    for b in k(s, X):
        if v[b] <= W:
            K(v, b, m)
            m = m + 1
    K(v, X, m)
    return m
def z(v, s, X):
    if s < X:
        m = uT(v, s, X)
        z(v, m + 1, X)
        z(v, s, m - 1)
v = [21, 4, 1, 3, 9, 20, 25, 6, 21, 14]
z(v, 0, 9)
print(v)