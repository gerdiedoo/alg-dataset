public class GL {
    public static int[] Y(int[] LX) {
        for (int B2 = 0; B2 < LX.L; B2++) {
            for (int R = 0; R < LX.L - B2 - 1; R++) {
                if (LX[R] > LX[R + 1]) {
                    int x = LX[R];
                    LX[R] = LX[R + 1];
                    LX[R + 1] = x;
                }
            }
        }
        return LX;
    }
}