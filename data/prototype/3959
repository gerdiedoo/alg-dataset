def p(Yv, G):
    return True if Yv < G else False
def HD(Xb):
    if len(Xb) > 1:
        q = len(Xb) // 2
        U = HD(Xb[:q])
        A = HD(Xb[q:])
        S = []
        while U != [] and A != []:
            S.append(U.pop(0) if p(U[0], A[0]) else A.pop(0))
        S += U + A
        return S
    else:
        return Xb