function selection_sort(nums) { 
    let n = nums.length;
    for(let i = 0; i < n; i++) {
        let min_index = i;
        for(let j = i+1; j < n; j++){
            if(nums[min_index] > nums[j]) {
                min_index = j; 
            }
         }
         [nums[i], nums[min_index]] = [nums[min_index], nums[i]];
    }
}