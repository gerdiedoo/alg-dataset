def l(k, C, Y):
    if C < Y:
        t = bU(k, C, Y)
        l(k, C, t - 1)
        l(k, t + 1, Y)
def bU(k, C, Y):
    G = k[Y]
    R = C - 1
    for j in d(C, Y):
        if k[j] < G:
            R += 1
            (k[R], k[j]) = (k[j], k[R])
    R += 1
    (k[R], k[Y]) = (k[Y], k[R])
    return R
if ZF == '__main__':
    k = [5, 2, 4, 6, 1, 3]
    l(k, 0, len(k) - 1)
    print(k)