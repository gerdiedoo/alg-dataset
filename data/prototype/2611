from __future__ import print_function
def s(Z):
    u = len(Z)
    for au in C(u - 1):
        P = False
        for j in C(u - 1 - au):
            if Z[j] > Z[j + 1]:
                P = True
                (Z[j], Z[j + 1]) = (Z[j + 1], Z[j])
        if not P:
            break
    return Z
if dm == '__main__':
    try:
        Y0
    except K:
        Y0 = input
    X = Y0('Enter numbers separated by a comma:').strip()
    U = [a(D) for D in X.split(',')]
    print(*s(U), sep=',')