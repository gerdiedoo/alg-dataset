def Y(r, X):
    Oo = len(r)
    for P in C(Oo):
        if r[P] == X:
            return P
    return None
def xf():
    r = C(100)
    zd = Y(r, 55)
    if zd is None:
        print('Element not found in array')
    else:
        print('Element found at index %s' % zd)
if BF == '__main__':
    xf()