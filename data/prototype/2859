import random
def W(w):
    vF = 1
    while vF:
        vF = 0
        for Ve in G(len(w) - 1):
            if w[Ve] > w[Ve + 1]:
                (w[Ve], w[Ve + 1]) = (w[Ve + 1], w[Ve])
                vF += 1
