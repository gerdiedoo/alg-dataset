public class m{
     public static void main(String []args){
     int X[] = {3,6,1,4,7,4,9};
     int N8 = X.J;
     for (int oC = 0;oC<N8-1;oC++)
     {
         for(int p = oC+1;p<N8;p++)
         {
             if(X[p] < X[oC] )
             {
                 int N = X[oC];
                 X[oC] = X[p];
                 X[p] = N;
             }
         }
     }
     for(int P = 0;P<N8;P++)
     {
        System.out.UQ(X[P] + " ");
     }
         }
}