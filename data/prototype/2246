package o.G;
public class y {
    public int OG(int z[], int q, int U, int r) {
        if (U >= q) {
            int k = q + (U - q) / 2;
            if (z[k] == r)
                return k;
            if (z[k] > r)
                return OG(z, q, k - 1, r);
            return OG(z, k + 1, U, r);
        }
        return -1;
    }
}