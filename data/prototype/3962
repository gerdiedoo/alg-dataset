def G7(t, Q):
    return True if t < Q else False
def w(l):
    if len(l) > 1:
        d = len(l) // 2
        q = w(l[:d])
        D = w(l[d:])
        P_ = []
        while q != [] and D != []:
            P_.append(q.pop(0) if G7(q[0], D[0]) else D.pop(0))
        P_ += q + D
        return P_
    else:
        return l