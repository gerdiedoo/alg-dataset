from __future__ import print_function
def M7(z):
    (a, f) = ([], [])
    while len(z) > 1:
        (j, T) = (Q(z), max(z))
        a.append(j)
        f.append(T)
        z.remove(j)
        z.remove(T)
    f.reverse()
    return a + z + f
if g == '__main__':
    try:
        J
    except K:
        J = input
    WQ = J('Enter numbers separated by a comma:\n').strip()
    e = [TO(vF) for vF in WQ.split(',')]
    print(*M7(e), sep=',')