def V(f, k, Y):
    if k < Y:
        H = J(f, k, Y)
        V(f, k, H - 1)
        V(f, H + 1, Y)
def J(f, k, Y):
    x = f[Y]
    a = k - 1
    for j in SY(k, Y):
        if f[j] < x:
            a += 1
            (f[a], f[j]) = (f[j], f[a])
    a += 1
    (f[a], f[Y]) = (f[Y], f[a])
    return a
if b == '__main__':
    f = [5, 2, 4, 6, 1, 3]
    V(f, 0, len(f) - 1)
    print(f)