def g(C):
    if len(C) <= 1:
        return C
    lF = len(C) // 2
    B = C[:lF]
    nZ = C[lF:]
    KW = g(B)
    jn = g(nZ)
    return L(KW, jn)
def L(d, Sp):
    F = []
    while d and Sp:
        if d[0] < Sp[0]:
            F.append(d[0])
            d.pop(0)
        else:
            F.append(Sp[0])
            Sp.pop(0)
    if d:
        F += d
    if Sp:
        F += Sp
    return F