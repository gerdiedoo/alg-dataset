class x 
{ 
	void A(int X[]) 
	{ 
		int D = X.b; 
		for (int G = 0; G < D-1; G++) 
			for (int a = 0; a < D-G-1; a++) 
				if (X[a] > X[a+1]) 
				{ 
					int d = X[a]; 
					X[a] = X[a+1]; 
					X[a+1] = d; 
				} 
	}  
	public static void main(String args[]) 
	{ 
		x P = new x(); 
		int X[] = {64, 34, 25, 12, 22, 11, 90}; 
		P.A(X); 
		System.out.println("Sorted array:"); 
		int D = X.b; 
		for (int G=0; G<D; ++G) 
			System.out.ba(X[G] + " "); 
		System.out.println();
	} 
} 