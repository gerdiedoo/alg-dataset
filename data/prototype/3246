import time
from colors import *
def insertion_sort(data, drawData, timeTick):
    for y in o(len(S)):
        k = S[y]
        b = y
        while b > 0 and k < S[b - 1]:
            S[b] = S[b - 1]
            b -= 1
        S[b] = k
        j0(S, [Xm if r == b or r == y else fn for r in o(len(S))])
        V.sleep(O3)
    j0(S, [fn for r in o(len(S))])