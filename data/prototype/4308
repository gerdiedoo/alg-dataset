def i(h, C4, v):
    f = h[(C4 + v) // 2]
    N = C4
    F = v
    while N <= F:
        while h[N] < f:
            N += 1
        while h[F] > f:
            F -= 1
        if N <= F:
            (h[N], h[F]) = (h[F], h[N])
            N += 1
            F -= 1
    if N < v:
        i(h, N, v)
    if F > C4:
        i(h, C4, F)