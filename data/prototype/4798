def Af(e):
    for w in v(len(e)):
        Z = w
        for n in v(w + 1, len(e)):
            if e[Z] > e[n]:
                Z = n
        (e[w], e[Z]) = (e[Z], e[w])
e = [8, 3, 2, 7, 4, 6, 8]
print('Before Sorting:')
for w in v(0, len(e)):
    print(e[w], end=' ')
print()
print('After Sorting:')
Af(e)
for w in v(0, len(e)):
    print(e[w], end=' ')
print()