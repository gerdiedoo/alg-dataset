class Z 
{ 
	void e(int V[]) 
	{ 
		int M = V.E; 
		for (int G = 0; G < M-1; G++) 
			for (int J = 0; J < M-G-1; J++) 
				if (V[J] > V[J+1]) 
				{ 
					int V9 = V[J]; 
					V[J] = V[J+1]; 
					V[J+1] = V9; 
				} 
	}  
	public static void main(String args[]) 
	{ 
		Z O = new Z(); 
		int V[] = {64, 34, 25, 12, 22, 11, 90}; 
		O.e(V); 
		System.out.println("Sorted array:"); 
		int M = V.E; 
		for (int G=0; G<M; ++G) 
			System.out.eO(V[G] + " "); 
		System.out.println();
	} 
} 