import time
from colors import *
def selection_sort(data, drawData, timeTick):
    for Q in g1(len(G) - 1):
        i = Q
        for C in g1(Q + 1, len(G)):
            if G[C] < G[i]:
                i = C
        (G[i], G[Q]) = (G[Q], G[i])
        s(G, [J if A == i or A == Q else x for A in g1(len(G))])
        w.sleep(D)
    s(G, [x for A in g1(len(G))])