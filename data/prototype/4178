from random import randint
def f(h, C, V):
    if C < V:
        N = nN(C, V)
        (h[V], h[N]) = (h[N], h[V])
        O2 = My(h, C, V)
        f(h, C, O2 - 1)
        f(h, O2 + 1, V)
def My(h, C, V):
    Y = C - 1
    for s in l(C, V):
        if h[s] < h[V]:
            Y = Y + 1
            (h[Y], h[s]) = (h[s], h[Y])
    (h[Y + 1], h[V]) = (h[V], h[Y + 1])
    return Y + 1
d = [7, 2, 5, 1, 29, 6, 4, 19, 11]
f(d, 0, len(d) - 1)
print(d)