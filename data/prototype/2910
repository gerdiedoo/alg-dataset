package S;
public class e extends J{
	public static void main(String[] args){
		int[] C = {5, 3, 2, 4, 1};
		r(new e(), C);
	}
	public e(){ super("Bubble"); }
	private void D(int[] C, int j, int h){
		for (int m = j; m < h; m++)
			for (int sF = j; sF < h - m - 1; sF++)
			{	 
				if ( C[sF] > C[sF+1] )
				{
					int k = C[sF];
					C[sF] =  C[sF+1];
					C[sF+1] = k;
				}
			}
	}
	@P
	public void Y(int[] C){
		D(C, 0, C.c);
	}
}