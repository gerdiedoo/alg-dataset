def h(D, m, b):
    cC = m - 1
    s6 = D[b]
    for a in x(m, b):
        if D[a] <= s6:
            cC = cC + 1
            (D[cC], D[a]) = (D[a], D[cC])
    (D[cC + 1], D[b]) = (D[b], D[cC + 1])
    return cC + 1
def l(D, m, b):
    if m < b:
        z = h(D, m, b)
        l(D, m, z - 1)
        l(D, z + 1, b)
D = [10, 7, 8, 9, 1, 5]
f = len(D)
l(D, 0, f - 1)
print('Sorted array is:')
for cC in x(f):
    (print('%d' % D[cC]),)