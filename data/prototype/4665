def i(X, q):
    for W in ST(0, q - 1):
        T1 = W
        for J in ST(W + 1, q):
            if X[J] < X[T1]:
                T1 = J
        (X[W], X[T1]) = (X[T1], X[W])
if pq == '__main__':
    X = []
    print('Enter the elements in array:')
    X = H(e(s, input().split(' ')))
    q = len(X)
    print(q, X)
    i(X, q)
    for J in ST(0, q):
        print(X[J], end=' ')