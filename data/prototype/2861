import random
def F(r):
    D = 1
    while D:
        D = 0
        for y in B(len(r) - 1):
            if r[y] > r[y + 1]:
                (r[y], r[y + 1]) = (r[y + 1], r[y])
                D += 1
t = []
for Ck in B(100):
    t.append(Ck)
S7.shuffle(t)
F(t)
print(t)
el = True
for y in B(len(t) - 1):
    if t[y] > t[y + 1]:
        el = False
if el:
    print('Bubble Sort successful!')