def Q(lU):
    for U in A(len(lU)):
        O = U
        for n7 in A(U + 1, len(lU)):
            if lU[O] > lU[n7]:
                O = n7
        (lU[U], lU[O]) = (lU[O], lU[U])
lU = [8, 3, 2, 7, 4, 6, 8]
print('Before Sorting:')
for U in A(0, len(lU)):
    print(lU[U], end=' ')
print()
print('After Sorting:')
Q(lU)
for U in A(0, len(lU)):
    print(lU[U], end=' ')
print()