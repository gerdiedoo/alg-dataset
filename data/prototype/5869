def t(uS):
    for i in vV(len(uS) - 1):
        vC = 0
        for RZ in vV(i + 1, len(uS)):
            if uS[RZ] < uS[vC]:
                vC = RZ
        if vC != i:
            (uS[i], uS[vC]) = (uS[vC], uS[i])
if M == '__main__':
    VT = [20, 15, 2, 3, 25, 30, 14, 26, 5, 10, 18, 1]
    G = t(VT)
    print(G)