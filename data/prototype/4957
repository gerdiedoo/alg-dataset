public class s{
     public static void main(String []args){
     int Y[] = {3,6,1,4,7,4,9};
     int L = Y.O;
     for (int OY = 0;OY<L-1;OY++)
     {
         for(int t = OY+1;t<L;t++)
         {
             if(Y[t] < Y[OY] )
             {
                 int Vr = Y[OY];
                 Y[OY] = Y[t];
                 Y[t] = Vr;
             }
         }
     }
     for(int D = 0;D<L;D++)
     {
        System.out.N(Y[D] + " ");
     }
         }
}