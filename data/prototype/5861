def M(g):
    W4 = len(g)
    for L in XA(W4 - 1):
        Q = L
        for ZQ in XA(Q + 1, W4):
            if g[ZQ] < g[Q]:
                Q = ZQ
        if L != Q:
            (g[L], g[Q]) = (g[Q], g[L])
if h == '__main__':
    v = [[89, 78, 61, 53, 23, 21, 17, 12, 9, 7, 6, 2, 1], [], [1, 5, 8, 9], [234, 3, 1, 56, 34, 12, 9, 12, 1300], [78, 12, 15, 8, 61, 53, 23, 27], [5]]
    for z in v:
        M(z)
        print(z)