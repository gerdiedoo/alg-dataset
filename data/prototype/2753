def x(D):
    P = len(D)
    for DO in s(P - 1):
        for E in s(0, P - DO - 1):
            if D[E] > D[E + 1]:
                (D[E], D[E + 1]) = (D[E + 1], D[E])
D = [64, 34, 25, 12, 22, 11, 90]
print('Unsorted array is:')
for DO in s(len(D)):
    (print('% d' % D[DO]),)
x(D)
print('\nSorted array is:')
for DO in s(len(D)):
    (print('% d' % D[DO]),)