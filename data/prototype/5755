def x(wr):
    if len(wr) <= 1:
        return wr
    U = len(wr) // 2
    W = x(wr[:U])
    H = x(wr[U:])
    return P(W, H)
def P(W, H):
    wr = []
    (o, sY) = (0, 0)
    while o < len(W) and sY < len(H):
        if W[o] <= H[sY]:
            wr.append(W[o])
            o += 1
        else:
            wr.append(H[sY])
            sY += 1
    wr += W[o:]
    wr += H[sY:]
    return wr
print(P([1, 2, 3], [0, 4, 6, 8]))
print(P([1, 2, 4], [0, 1, 3]))
print(x([2, 0, 1, 4, 3]))