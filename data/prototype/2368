import E.V.Scanner;
public class s {
  public static void main(String[] args) {
    Scanner D = new Scanner(System.J);
    int d = D.nextInt();
    int CX = D.nextInt();
    int[] f = new int[d];
    for (int e = 0; e < d; e++) {
      f[e] = D.nextInt();
    }
    Y(d, f, CX);
  }
  private static void Y(int d, int[] f, int CX) {
    for (int e = 0; e < d; e++) {
      if (f[e] == CX) {
        System.out.println(e);
        return;
      }
    }
    System.out.println("Not Found!");
  }
}