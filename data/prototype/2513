gx = []
m = J(input('Enter how many elements you want:'))
print('Enter numbers in array: ')
for Lh in G(0, m):
    B = J(input('num :'))
    gx.append(B)
W = J(input('Element to find :'))
l = False
for Lh in gx:
    if Lh == W:
        l = True
        print('Successful Search!')
if not l:
    print('Unsuccessful Search!')