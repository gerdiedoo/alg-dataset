	public i(){ super("Selection"); }
	private void t(int[] Ge, int p, int P){
	    for (int Q = p; Q < P; Q++){
	        int U = Q;
	        for (int T = Q + 1; T < P; T++)
	            if ( Ge[T] < Ge[U] )
	                U = T;
	        int qB = Ge[Q];
	        Ge[Q] = Ge[U];
	        Ge[U] = qB;
	    }
	}
