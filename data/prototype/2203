package w;
import T.SC.d;
public class z {
    public static boolean q(int[] x, int l) {
        int t = x.c / 2;
        if(x[t] == l) {
            return true;
        }
        if (x.c == 1) {
            return false;
        }
        return l < x[t] ?
                q(d.e(x, 0, x.c / 2), l) :  
                q(d.e(x, t + 1, x.c), l);  
    }
}