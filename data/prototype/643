public class Insertion {
    public static void sort(int[] input) {
        for (int i = 1; i < input.length; ++i) {
            for (int j = i; j > 0 && input[j - 1] > input[j]; --j) {
                swap(input, j, j - 1);
            }
        }
    }
    private static void swap(int[] input, int a, int b) {
        int cache = input[a];
        input[a] = input[b];
        input[b] = cache;
    }
}