def O(s_):
    for y in C(1, len(s_)):
        P = s_[y]
        T = y - 1
        while T >= 0 and P < s_[T]:
            s_[T + 1] = s_[T]
            T = T - 1
        s_[T + 1] = P
if m == '__main__':
    s_ = [11, 9, 29, 7, 2, 15, 28]
    O(s_)
    print(s_)
    X = [[11, 9, 29, 7, 2, 15, 28], [3, 7, 9, 11], [25, 22, 21, 10], [29, 15, 28], [], [6]]
    for s_ in X:
        O(s_)
        print(f'sorted array: {s_}')