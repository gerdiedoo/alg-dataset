def t(v):
    for O in Jg(len(v) - 1):
        CI = 0
        for Hn in Jg(O + 1, len(v)):
            if v[Hn] < v[CI]:
                CI = Hn
        if CI != O:
            (v[O], v[CI]) = (v[CI], v[O])
if b == '__main__':
    w = [20, 15, 2, 3, 25, 30, 14, 26, 5, 10, 18, 1]
    P = t(w)
    print(P)